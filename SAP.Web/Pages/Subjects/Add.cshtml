@page
@model AddModel

<h2 class="mt-2 mx-5">Add Subject</h2>
<div class="card col-md-10 mx-5">
    <form method="post">
    <div class="card-header"></div>
    <div class="card-body">
            <div class="col">
                <div class="mb-3 form-floating">
                    <input type="text" asp-for="Subjects.Name" class="form-control" />
                    <label>Name</label>
                </div>
            </div>
    </div>
    <div class="card-footer">
        <button class="btn btn-success"><i class="fa-solid fa-floppy-disk"></i> Submit</button>
    </div>
    </form>
</div>
